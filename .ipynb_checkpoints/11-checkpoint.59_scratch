    "class conv:\n",
    "    def __init__(self, filt_num, dim_fil):\n",
    "        self.filt_num = filt_num\n",
    "        self.dim_fil = dim_fil\n",
    "        self.fil_convolut = np.random.randn(filt_num, dim_fil, dim_fil)/(dim_fil * dim_fil)\n",
    "\n",
    "    def image_region(self, img_var):\n",
    "        h, w = img_var.shape\n",
    "        self.img_var = img_var\n",
    "        for j in range(h - self.dim_fil + 1):\n",
    "            for k in range(w - self.dim_fil + 1):\n",
    "                img_var_patch = img_var[ j : (j + self.dim_fil), k : (k + self.dim_fil)]\n",
    "                yield img_var_patch, j, k\n",
    "\n",
    "    def forward_propagation(self, img_var):\n",
    "        h, w = img_var.shape\n",
    "        conv_out = np.zeros((h - self.dim_fil + 1, w - self.dim_fil + 1, self.filt_num))\n",
    "        for img_var_patch, i, j in self.image_region(img_var):\n",
    "            conv_out[i, j] = np.sum(img_var_patch * self.fil_convolut, axis = (1,2))\n",
    "        return conv_out\n",
    "\n",
    "    def back_propagation(self, dl_dout, learning_rate):\n",
    "        dl_df_params = np.zeros(self.fil_convolut.shape)\n",
    "        for img_var_patch, i, j in self.image_region(self.img_var):\n",
    "            for k in range(self.filt_num):\n",
    "                dl_df_params[k] += img_var_patch*dl_dout[i, j, k]\n",
    "\n",
    "            self.fil_convolut -= learning_rate*dl_df_params\n",
    "            return dl_df_params"
   ]
  },
