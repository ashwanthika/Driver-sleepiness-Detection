    "class max_pool:\n",
    "    def __init__(self, dim_fil):\n",
    "        self.dim_fil = dim_fil\n",
    "\n",
    "    def image_region(self, img_var):\n",
    "        new_height = img_var.shape[0] // self.dim_fil\n",
    "        new_width = img_var.shape[1] // self.dim_fil\n",
    "        self.img_var = img_var\n",
    "\n",
    "        for i in range(new_height):\n",
    "            for j in range(new_width):\n",
    "                img_var_patch = img_var[(i*self.dim_fil) :(i*self.dim_fil + self.dim_fil), (j*self.dim_fil) : (j*self.dim_fil + self.dim_fil)]\n",
    "                yield img_var_patch, i, j\n",
    "\n",
    "    def forward_propagation(self, img_var):\n",
    "        h, w, filt_num = img_var.shape\n",
    "        output = np.zeros((h // self.dim_fil, w // self.dim_fil, filt_num))\n",
    "\n",
    "        for img_var_patch, i, j in self.image_region(img_var):\n",
    "            output[i, j] = np.amax(img_var_patch, axis = (0,1))\n",
    "\n",
    "        return output\n",
    "\n",
    "    def back_propagation(self, dl_dout):\n",
    "        dl_dmax_pool = np.zeros(self.img_var.shape)\n",
    "        for img_var_patch, i, j in self.image_region(self.img_var):\n",
    "            h, w, filt_num = img_var_patch.shape\n",
    "            maximum_val = np.amax(img_var_patch, axis=(0,1))\n",
    "\n",
    "            for i1 in range(h):\n",
    "                for j1 in range(w):\n",
    "                    for k1 in range(filt_num):\n",
    "                        if img_var_patch[i1, j1, k1] == maximum_val[k1]:\n",
    "                            dl_dmax_pool[i*self.dim_fil + i1, j*self.dim_fil + j1, k1] = dl_dout[i, j, k1]\n",
    "\n",
    "            return dl_dmax_pool"
   ]
